{
	"plugins": ["spellcheck"],
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module"
	},
	"env": {
		"node": true,
		"es6": true,
		"commonjs": true,
		"jest": true,
		"mocha": true
	},
	"rules": {
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"array-bracket-spacing": ["error", "never"],
		"arrow-spacing": ["error"],
		"brace-style": ["error"],
		"comma-spacing": ["error"],
		"curly": ["error"],
		"dot-notation": ["error"],
		"eqeqeq": ["error"],
		"keyword-spacing": ["error"],
		"linebreak-style": ["error", "unix"],
		"max-len": [
			"error",
			{
				"code": 150,
				"ignoreTrailingComments": true,
				"ignoreUrls": true,
				"ignoreStrings": true,
				"ignoreTemplateLiterals": true,
				"ignoreRegExpLiterals": true
			}
		],

		"no-console": ["off"],
		"no-mixed-spaces-and-tabs": ["error"],
		"no-shadow": ["error"],
		"no-throw-literal": ["error"],
		"no-trailing-spaces": ["error"],
		"no-useless-call": ["error"],
		"no-unused-vars": [
			"error",
			{
				"args": "none"
			}
		],
		"no-var": ["error"],
		"no-with": ["error"],
		"operator-linebreak": ["error"],
		"prefer-arrow-callback": ["error"],
		"prefer-const": ["error"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"spaced-comment": ["error", "always"],
		"space-before-blocks": [
			"error",
			{
				"functions": "always",
				"keywords": "always",
				"classes": "always"
			}
		],
		"space-infix-ops": ["error"],
		"space-in-parens": ["error", "never"],
		"yoda": ["error"],

		"padded-blocks": "off",
		"no-unexpected-multiline": "error",
		"no-unused-expressions": "error",
		"no-unused-labels": "error",
		"comma-dangle": "error",
		"no-cond-assign": ["error"],
		"no-undef": "error",
		"overrides": [
			{
				"files": ["*.spec.js", "**/app/test/**"],
				"rules": {
					"no-unused-expressions": "off"
				}
			}
		],
		"spellcheck/spell-checker": [
			2,
			{
				"comments": true,
				"strings": true,
				"identifiers": false,
				"lang": "en_US",
